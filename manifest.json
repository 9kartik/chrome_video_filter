{
  "name": "Alter VIDEOS",
  "description": "Apply live video filters",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
      "default_title": "Apply live video filters",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*",
            "file://*"
            ],
        "js": ["content.js"],
        "run_at": "document_end"         // pay attention to this line
        }
    ], 
  "manifest_version": 2
}
